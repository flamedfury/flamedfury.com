<ul class="flow">
  {% for item in changelog.changeLog | reverse | limit(05) %}
    <li>
      {% set definedDate = item.date %}
      <span class="updates-date">{% include "partials/date.njk" %}</span>: {{ item.update }}
    </li>
  {% endfor %}
</ul>

<p class="text-center" style="font-size: var(--size-step-0);">
  <a class="updates-link" href="/updates/">Website changelog</a>
</p>